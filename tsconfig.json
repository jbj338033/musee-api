{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"types": ["bun"],
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"esModuleInterop": true,
		"forceConsistentCasingInImports": true,
		"resolveJsonModule": true,
		"isolatedModules": true
	},
	"include": ["src"]
}
